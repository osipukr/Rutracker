{
  "Logging": {
    "PathFormat": "Logs/log-{Date}.txt",
    "OutputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} | [{Level:u}] | {Message}{NewLine}{Exception}",
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Error"
    }
  },
  "CacheOptions": {
    "CacheDuration": "00:01:00"
  },
  "FileStorageOptions": {
    "FileMimeTypes": [],
    "ImageMimeTypes": [
      "image/png",
      "image/svg",
      "image/jpeg",
      "image/gif",
      "image/jpg"
    ],
    "FileMaxLength": 52428800,
    "ImageMaxLength": 2097152,
    "ImageContainer": "images",
    "TorrentContainer": "torrent-{0}",
    "UserImageName": "profile-image-{0}",
    "TorrentImageName": "torrent-image-{0}",
    "TorrentFileName": "file-{0}"
  }
}