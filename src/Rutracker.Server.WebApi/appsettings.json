{
  "Logging": {
    "PathFormat": "Logs/log-{Date}.txt",
    "OutputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} | [{Level:u}] | {Message}{NewLine}{Exception}",
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Error"
    }
  },
  "CacheOptions": {
    "CacheDuration": "00:01:00"
  },
  "UserImageOptions": {
    "MimeTypes": [
      "image/png",
      "image/svg",
      "image/jpeg",
      "image/gif",
      "image/jpg"
    ],
    "MaxBytesLength": 2097152,
    "FileName": "profile-image"
  },
  "ResponseCompressionSettings": {
    "MimeTypes": [
      "application/octet-stream",
      "application/wasm",
      "application/json",
      "image/png",
      "image/svg",
      "image/jpeg",
      "image/gif",
      "image/jpg"
    ]
  }
}