<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <OutputType>Exe</OutputType>
    <LangVersion>8</LangVersion>
    <RazorLangVersion>3.0</RazorLangVersion>
    <Authors>Roman Osipuk</Authors>
    <Product>Rutracker</Product>
    <RepositoryUrl>https://github.com/osipukr/Rutracker-Blazor</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Blazored.LocalStorage" Version="2.0.7" />
    <PackageReference Include="Blazored.Modal" Version="2.0.5" />
    <PackageReference Include="MatBlazor" Version="1.5.2" />
    <PackageReference Include="Microsoft.AspNetCore.Blazor" Version="3.0.0-preview7.19365.7" />
    <PackageReference Include="Microsoft.AspNetCore.Blazor.Build" Version="3.0.0-preview7.19365.7" PrivateAssets="all" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
    <PackageReference Include="System.IdentityModel.Tokens.Jwt" Version="5.5.0" />
    <PackageReference Include="Tewr.Blazor.FileReader" Version="0.13.19207" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Rutracker.Shared.Models\Rutracker.Shared.Models.csproj" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="clientsettings.json" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Shared\Category\" />
    <Folder Include="Shared\Subcategory\" />
  </ItemGroup>

</Project>